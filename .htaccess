RewriteEngine On 
RewriteCond %{REQUEST_URI} / [NC]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule    ^sign-in/?$    login-page.php    [NC,L]
RewriteRule    ^sign-out/?$    logout.php    [NC,L]
RewriteRule    ^all-clients/?$    clients-management.php    [NC,L]
RewriteRule    ^info-clients/?$    clients-details.php    [NC,L]
RewriteRule    ^new-clients/?$    client-add-new-form.php    [NC,L]
RewriteRule    ^all-policy/?$    policy-management.php    [NC,L]
RewriteRule    ^info-policy/?$    policy-details.php    [NC,L]
RewriteRule    ^all-users/?$    user-management.php    [NC,L]
RewriteRule    ^setting-user/?$    user-setting.php    [NC,L]



